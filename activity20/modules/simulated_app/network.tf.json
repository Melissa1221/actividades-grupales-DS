{
  "variable": [
    {
      "name": [
        {
          "type": "string",
          "default": "application-server",
          "description": "Nombre de la aplicación del servidor"
        }
      ]
    },
    {
      "server1_name": [
        {
          "type": "string",
          "default": "app1",
          "description": "Nombre del primer servidor para el balanceador"
        }
      ]
    },
    {
      "server2_name": [
        {
          "type": "string",
          "default": "app2",
          "description": "Nombre del segundo servidor para el balanceador"
        }
      ]
    },
    {
      "network": [
        {
          "type": "string",
          "default": "local-network",
          "description": "Nombre de la red local"
        }
      ]
    },
    {
      "port": [
        {
          "type": "number",
          "default": 8080,
          "description": "Puerto de la aplicación"
        }
      ]
    }
  ]
